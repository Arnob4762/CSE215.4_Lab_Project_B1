<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="337.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RegistrationController">
    <children>
        <Label layoutX="98.0" layoutY="38.0" prefHeight="34.0" prefWidth="372.0" text="Please give following information to register" textFill="#226068">
            <font>
                <Font name="System Bold Italic" size="17.0" />
            </font>
        </Label>
        <TextField fx:id="txtuser" layoutX="200.0" layoutY="110.0" prefHeight="25.0" prefWidth="180.0" promptText="username" />
        <Label layoutX="98.0" layoutY="110.0" prefHeight="25.0" prefWidth="77.0" text="Username">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="98.0" layoutY="150.0" prefHeight="21.0" prefWidth="77.0" text="Password">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="98.0" layoutY="188.0" prefHeight="25.0" prefWidth="96.0" text="Email Address">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <PasswordField fx:id="txtpass" layoutX="200.0" layoutY="148.0" prefHeight="25.0" prefWidth="180.0" promptText="password" />
        <TextField fx:id="txtemail" layoutX="200.0" layoutY="188.0" prefHeight="25.0" prefWidth="180.0" promptText="email@" />
        <Button fx:id="SigninButton" layoutX="214.0" layoutY="268.0" mnemonicParsing="false" onAction="#SignIn" prefHeight="25.0" prefWidth="152.0" text="Sign in!">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button fx:id="saveButton" layoutX="259.0" layoutY="224.0" mnemonicParsing="false" onAction="#Save" text="Save">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button fx:id="BackButton" layoutY="2.0" mnemonicParsing="false" onAction="#Back" prefHeight="25.0" prefWidth="71.0" text="Back" textFill="#ae4545">
            <font>
                <Font name="System Italic" size="12.0" />
            </font>
        </Button>
        <Label fx:id="status" layoutX="102.0" layoutY="74.0" prefHeight="25.0" prefWidth="354.0" />
    </children>
</AnchorPane>

